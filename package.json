	],
	"author": "@aMediocreDad | Filip Ambrosius",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/aMediocreDad/foundry-db-packer/issues"
	},
	"homepage": "https://github.com/aMediocreDad/foundry-db-packer#readme",
	"dependencies": {
		"@actions/core": "1.10.0",
		"@actions/exec": "1.1.1"
	},
	"devDependencies": {
		"@changesets/cli": "2.26.1",
		"@types/js-yaml": "4.0.5",
		"@types/node": "20.2.5",
		"builtin-modules": "3.3.0",
		"chalk": "5.2.0",
		"esbuild": "0.17.19",
		"js-yaml": "4.1.0",
		"nedb-promises": "6.2.1",
		"typescript": "5.0.4",
